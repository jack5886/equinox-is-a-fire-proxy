<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>Equinox Mobile</title>

  <link rel="stylesheet" href="/assets/css/style.css" />
  <link rel="stylesheet" href="/assets/css/games.css" />
  <link rel="icon" type="image/x-icon" href="/assets/favicon.ico">


  <script src="http://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js"></script>
  <script src="/uv/uv.bundle.js"></script>
  <script src="/uv/uv.config.js"></script>

  <script src="/assets/js/index.js" defer></script>
  <script src="/assets/js/particles.js"></script>
  <script src="/assets/js/cursor.js" defer></script>
  <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-1551126437976866"
    crossorigin="anonymous"></script>

</head>

<body>

  <div class="topbar" style="width: 100%;">

    <div class="left-buttons">

      <a id="exit" onclick="exit()" style="display: none">
        <span style="font-weight: bold;">Exit</span>
      </a>
      <a id="homebtn" style="display: none" href="/"
        class="<%= path === '/' ? 'nav-active' : 'nav-btn' %>"><span></span></a>
    </div>
    <form id="form" class="center-form">
      <input autofocus autocomplete="off" class="search" id="search" placeholder="What's on your mind?" required />
      <button type="submit" class="submit-button">
        <i class="fas fa-search searchicon"></i>
      </button>
    </form>
    <div class="right-section" style="display: none">
      <a class="nav-btn" href="/s" style="display: flex; justify-content: flex-end; margin-left: auto;">Settings</a>
      <a id="time" class="nav-btn" style="display: flex; justify-content: flex-end; margin-left: auto;">00:00PM</a>
    </div>
  </div>
  <svg height="100%" width="100%" style="position: fixed; z-index: 0;"></svg>

  <div id="align">
    <iframe id="ifr"></iframe>
  </div>

  <div class="alignment-container-1" style="margin-top:10%;">
    <div class="header-container">
      <div class="logo-image">
        <img src="assets/eq.png" alt="Logo" id="spinning" width="100%">
      </div>
    </div>
    
    <div class="flex-center desc" style="text-align: center;">
      <p id="creation-info">
      </p>
    </div>


    <footer>
      <a title="discord" href="https://discord.gg/H7JqRwykhk">Discord</a>
      <a title="version" href="https://github.com/wrndxyz/Equinox" style="color: rgb(80, 80, 80)">
        Equinox Mobile v1.0
      </a>
      <a title="github" href="https://github.com/wrndxyz/">WRND</a>
    </footer>
  </div>
  <script src="/assets/js/games.js"></script>
  <script src="https://d3js.org/d3.v4.min.js"></script>
  <script src="https://d3js.org/d3-hexbin.v0.2.min.js"></script>
  <script src="/assets/js/mobile-proxy.js" defer></script>
  <script type='text/javascript'
    src='//pl22191078.toprevenuegate.com/10/4f/de/104fdeafb2e5efb53ca806fb837ff878.js'></script>
  <script src="/assets/js/mobile-home.js"></script>
    

</body>

</html>